{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages

		{
            "path" : "pages/tabbar/index/index",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "列表",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/tabbar/completed/completed",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "已完成",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/tabbar/calendar/calendar",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "日历",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/tabbar/person/person",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "个人",
                "enablePullDownRefresh": false
            }
            
        }
    ],

	"globalStyle": {
		// #ifdef H5
		"navigationStyle": "custom",
		// #endif
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "ToDoList",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"app-plus": {
			"background": "#efeff4"
		}
	},
	"tabBar": {
		"color": "#7A7E83",
		"borderStyle": "white",
		"selectedColor": "#d06769",
		"list": [{
				"pagePath": "pages/tabbar/index/index",
				"iconPath": "./static/tabbar/preList.png",
				"selectedIconPath": "./static/tabbar/lastList.png",
				"text":"列表"
			},
			{
				"pagePath": "pages/tabbar/completed/completed",
				"iconPath": "./static/tabbar/preCompleted.png",
				"selectedIconPath": "./static/tabbar/lastCompleted.png", 
				"text": "已完成"
				
			},
			{

				"pagePath": "pages/tabbar/calendar/calendar",
				"iconPath": "./static/tabbar/preCalendar.png",
				"selectedIconPath": "./static/tabbar/lastCalendar.png",
				"text": "日历"

			},

			{
				"pagePath": "pages/tabbar/person/person",
				"iconPath": "./static/tabbar/prePerson.png",
				"selectedIconPath": "./static/tabbar/lastPerson.png",
				"text": "个人"
			}
		]
	},

	"uniIdRouter": {}
}
